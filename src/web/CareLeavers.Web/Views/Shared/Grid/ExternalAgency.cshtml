@model CareLeavers.Web.Models.Content.ExternalAgency

<div class="dfe-box box-ext">
    <div class="box-icon">
        @if (Model.Logo != null)
        {
            <contentful-image asset="@Model.Logo"></contentful-image>
        }
    </div>
    <div class="box-content">
        <h3 class="govuk-heading-s">@Model.Name</h3>
        @if (Model.Content != null)
        {
            <gds-contentful-rich-text document="@Model.Content"/>
        }
    </div>
</div>